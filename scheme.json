{
  "name": "NodeAppDirector",

  "buildDir": "./",

  "compilerOptions": {
    "compilationLevel": "ADVANCED_OPTIMIZATIONS",
    "warningLevel": "VERBOSE",
    "language": "ECMASCRIPT5",
    "formatting": ""
  },

  "srcDir": "lib",
  "rootNamespace": "app",

  "modules": {
    "app1": {
      "src": [
        "app1.js"
      ]
    },

    "app2": {
      "src": [
        "app2.js"
      ]
    }
  },

  "externsDir": "externs",

  "deps": {
    "justFM": {
      "repo": "https://github.com/kalachevmax/justFM",
      "type": "git",
      "version": "*"
    }
  }
}